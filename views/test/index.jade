doctype html
html
  head
    title testing
    meta(charset='utf-8')
    script(src="/jslib/jquery.js")
    style.
        html{
          height: 100%;
        }
        body{
          height: 100%;
          padding:0;
          margin:0;
          font-family: "微软雅黑","宋体";
          font-size: 12px;
          color: #666;
        }
        *{
          box-sizing: border-box;
        }
        .clear{
          clear: both;
        }
        .row {
            display: table;
            content: " ";
        }
        .col-1, .col-2, .col-3, .col-4, .col-5, .col-6, .col-7, .col-8, .col-9, .col-10, .col-11, .col-12 {
          float: left;
        }
        .col-12 {
          width: 100%;
        }
        .col-11 {
          width: 91.66666667%;
        }
        .col-10 {
          width: 83.33333333%;
        }
        .col-9 {
          width: 75%;
        }
        .col-8 {
          width: 66.66666667%;
        }
        .col-7 {
          width: 58.33333333%;
        }
        .col-6 {
          width: 50%;
        }
        .col-5 {
          width: 41.66666667%;
        }
        .col-4 {
          width: 33.33333333%;
        }
        .col-3 {
          width: 25%;
        }
        .col-2 {
          width: 16.66666667%;
        }
        .col-1 {
          width: 8.33333333%;
        }
        .wraper{
          position: relative;
          height: 100%;
        }
        .header{
          position: fixed;
          width: 100%;
          height: 37px;
          line-height: 37px;
          top: 0;
          left: 0;
          background: url(/img/white_bg.png) repeat-x;
          z-index: 61;
        }
        .footer{
          width: 100%;
          height: 50px;
          background-color: #1f2128;
        }
        .content{
          height: 100%;
          padding-bottom: 90px;
          padding-top: 37px;
        }
        .container{
          position: relative;
          min-height: 100%;
          //- border: 1px solid black;
          margin: 20px 0;
        }

        .memberBar {
            position: relative;
            padding: 0;
            margin: 0 auto;
            //- width: 960px;
            text-align: left;
            color: #000;
        }
        #memberBar {
            width: 1200px;
        }
        .memberBar a, .memberBar span, .memberBar div {
            color: #7c7c7c;
        }
        .memberBar .right {
            text-align: right;
            padding-right: 10px;
            color: #555;
        }
        .memberBar .right a {
            float: right;
            text-decoration: none;
        }
        .memberBar .right .memberOption {
            padding: 0 20px;
            width: auto;
            height: 24px;
            line-height: 23px;
            text-align: center;
            margin: 6px 6px auto 0;
        }
        .memberBar a:hover {
            color: #1b7ad1
        }
        .memberBar .memberReg {
            margin-right: 10px !important;
            border: 1px solid #2a96ff;
            background-color: #45a2ff;
            color: #fff;
        }
        .memberBar .memberReg:hover {
            border: 1px solid #1489fc;
            background-color: #2594ff;
            color: #fff
        }
        .memberBar .memberLogin {
            border: 1px solid #dadada;
        }
        .memberBar .memberLogin:hover {
            border: 1px solid #2a96ff
        }
  body
    div.wraper
        div.header
            div#memberBar.memberBar.row
                div.col-6.left xiao
                div.col-6.right
                    a.memberOption.memberReg(href="javascript:void(0)") 注册
                    a.memberOption.memberLogin(href="javascript:void(0)") 登录
        div.content
            div.container
                div(style="height:100px") ddd
                div(style="height:100px") ddd
                div(style="height:100px") ddd
                div(style="height:100px") ddd
                div(style="height:100px") ddd
                div(style="height:100px") ddd
                div(style="height:100px") ddd
                div(style="height:100px") ddd
                div(style="height:100px") ddd
                div(style="height:100px") ddd
            div.footer

